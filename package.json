		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.10",
		"normalize.css": "^8.0.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"sakura.css": "^1.5.0",
		"svelte": "5.0.0-next.78",
		"svelte-check": "^3.6.7",
		"sveltekit-superforms": "^2.10.0",
